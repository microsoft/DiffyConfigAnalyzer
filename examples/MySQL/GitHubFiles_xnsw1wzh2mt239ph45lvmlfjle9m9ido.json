{
  "mysqld": {
    "datadir": "/var/lib/mysql",
    "socket": "/var/lib/mysql/mysql.sock",
    "bind-address": "127.0.0.1",
    "symbolic-links": "0",
    "user": "mysql",
    "performance_schema": "",
    "slow_query_log": "",
    "slow_query_log-file": "/var/log/mysql-slow.sql",
    "long_query_time": "5"
  },
  "mysqld_safe": {
    "log-error": "/var/log/mysqld.log",
    "pid-file": "/var/run/mysqld/mysqld.pid"
  }
}
{
  "mysqld": {
    "datadir": "/var/lib/mysql",
    "socket": "/var/lib/mysql/mysql.sock",
    "user": "mysql",
    "symbolic-links": "0",
    "max_connections": "200",
    "max_user_connections": "30",
    "wait_timeout": "30",
    "interactive_timeout": "50",
    "long_query_time": "5",
    "innodb_file_per_table": ""
  },
  "mysqld_safe": {
    "log-error": "/var/log/mysqld.log",
    "pid-file": "/var/run/mysqld/mysqld.pid"
  }
}
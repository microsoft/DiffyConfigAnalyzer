{
  "client": {
    "port": "3306",
    "socket": "/tmp/mysql.sock"
  },
  "mysqld": {
    "pid-file": "/tmp/mysql.pid",
    "back_log": "500",
    "datadir": "/mtimer/server/mysql/data",
    "default-storage-engine": "InnoDB",
    "character_set_server": "utf8",

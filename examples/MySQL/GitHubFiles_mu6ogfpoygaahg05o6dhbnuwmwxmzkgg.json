    "slow-query-log": "1",
    "long_query_time": "5",
    "slow_query_log_file": "/mtimer/log/mysql/mysql_slow.log",
    "port": "3306",
    "socket": "/tmp/mysql.sock",
    "skip-external-locking": "",
    "key_buffer_size": "16M",
    "max_allowed_packet": "1M",
    "table_open_cache": "64",

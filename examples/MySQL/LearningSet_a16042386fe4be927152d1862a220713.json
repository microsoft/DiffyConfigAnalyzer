{
  "client": {
    "port": "3306",
    "socket": "/var/run/mysqld/mysqld.sock"
  },
  "mysqld_safe": {
    "socket": "/var/run/mysqld/mysqld.sock",
    "nice": "0"
  },
  "mysqld": {
    "user": "mysql",
    "socket": "/var/run/mysqld/mysqld.sock",
    "port": "3306",
    "basedir": "/usr",
    "datadir": "/var/lib/mysql",
    "tmpdir": "/tmp",
    "skip-external-locking": "",
    "bind-address": "127.0.0.1",
    "key_buffer": "32M",
    "max_allowed_packet": "16M",
    "thread_stack": "192K",
    "thread_cache_size": "8",
    "sort_buffer_size": "4M",
    "read_buffer_size": "4M",
    "myisam_sort_buffer_size": "16M",
    "myisam-recover": "BACKUP",
    "max_connections": "100",
    "table_cache": "32",
    "tmp_table_size": "128M",
    "query_cache_type": "1",
    "query_cache_size": "64M",
    "log_error": "/var/log/mysql/error.log",
    "expire_logs_days": "10",
    "max_binlog_size": "100M"
  },
  "mysqldump": {
    "quick": "",
    "quote-names": "",
    "max_allowed_packet": "16M"
  },
  "mysql": {},
  "isamchk": {
    "key_buffer": "16M",
    "!includedir /etc/mysql/conf.d/": ""
  }
}
{
  "client": {
    "socket": "/tmp/mysql.sock"
  },
  "mysqld": {
    "socket": "/tmp/mysql.sock",
    "skip-external-locking": "",
    "key_buffer_size": "16M",
    "max_allowed_packet": "1M",
    "table_open_cache": "256",
    "sort_buffer_size": "1M",
    "read_buffer_size": "1M",
    "read_rnd_buffer_size": "4M",
    "thread_cache_size": "8",
    "query_cache_size": "0",
    "skip-networking": "",
    "log-bin": "mysql-bin",
    "binlog_format": "mixed",
    "innodb_buffer_pool_size": "256M"
  },
  "mysqldump": {
    "quick": "",
    "max_allowed_packet": "16M"
  },
  "mysql": {
    "no-auto-rehash": ""
  },
  "myisamchk": {
    "key_buffer_size": "128M",
    "sort_buffer_size": "128M",
    "read_buffer": "2M",
    "write_buffer": "2M"
  },
  "mysqlhotcopy": {
    "interactive-timeout": ""
  },
  "mysqladmin": {
    "socket": "/tmp/mysql.sock"
  }
}
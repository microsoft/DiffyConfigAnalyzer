    "sort_buffer_size": "4M",
    "join_buffer_size": "8M",
    "thread_cache_size": "50",
    "thread_stack": "196K",
    "default-storage-engine": "InnoDB",
    "slow_query_log": "",
    "long_query_time": "5",

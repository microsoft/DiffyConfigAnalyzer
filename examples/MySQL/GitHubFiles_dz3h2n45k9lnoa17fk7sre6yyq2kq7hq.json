{
  "mysql": {
    "port": "3306",
    "socket": "/usr/local/var/run/mysql/mysql.sock",
    "default-character-set": "utf8"
  },
  "mysqld": {
    "user": "james",
    "default-storage-engine": "InnoDB",
    "socket": "/usr/local/var/run/mysql/mysql.sock",
    "pid-file": "/usr/local/var/run/mysql/mysql.pid",
    "character-set-server": "utf8",
    "collation-server": "utf8_general_ci",
    "skip-character-set-client-handshake": "1",
    "default-time-zone": "'+00:00'",
    "max-allowed-packet": "16M",
    "max-connect-errors": "1000000",
    "skip-name-resolve": "",
    "sql-mode": "STRICT_TRANS_TABLES,ERROR_FOR_DIVISION_BY_ZERO,NO_AUTO_CREATE_USER,NO_AUTO_VALUE_ON_ZERO,NO_ENGINE_SUBSTITUTION,NO_ZERO_DATE,NO_ZERO_IN_DATE,ONLY_FULL_GROUP_BY",
    "sysdate-is-now": "1",
    "innodb": "FORCE",
    "innodb-strict-mode": "1",
    "datadir": "/usr/local/var/mysql/",
    "log-bin": "/usr/local/var/mysql/mysql-bin",
    "expire-logs-days": "14",
    "sync-binlog": "1",
    "tmp-table-size": "32M",
    "max-heap-table-size": "32M",
    "query-cache-type": "0",
    "query-cache-size": "0",
    "max-connections": "100",
    "thread-cache-size": "50",
    "open-files-limit": "4096",
    "table-definition-cache": "1024",
    "table-open-cache": "2048",
    "innodb-flush-method": "O_DIRECT",
    "innodb-log-files-in-group": "2",
    "innodb-log-file-size": "64M",
    "innodb-flush-log-at-trx-commit": "1",
    "innodb-file-per-table": "1",
    "innodb-buffer-pool-size": "256M",
    "key-buffer-size": "32M",
    "myisam-recover": "FORCE,BACKUP",
    "log-queries-not-using-indexes": "1",
    "slow-query-log": "1",
    "slow-query-log-file": "/usr/local/var/log/mysql/mysql-slow.log",
    "log-error": "/usr/local/var/log/mysql/mysql-error.log"
  }
}
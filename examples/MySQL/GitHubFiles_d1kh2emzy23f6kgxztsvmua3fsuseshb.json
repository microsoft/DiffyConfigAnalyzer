    "innodb_log_file_size": "256M",
    "innodb_data_file_path": "ibdata1:256M:autoextend",
    "innodb_log_files_in_group": "3",
    "innodb_flush_method": "O_DIRECT",
    "max_heap_table_size": "64M",
    "tmp_table_size": "64M",
    "sort_buffer_size": "8M",
    "join_buffer_size": "8M",
    "query_cache_size": "0",
    "binlog_cache_size": "1M",
    "log-bin": "mysql-bin",
    "binlog_format": "mixed",
    "server-id": "1",
    "slow_query_log": "",
    "long_query_time": "2"

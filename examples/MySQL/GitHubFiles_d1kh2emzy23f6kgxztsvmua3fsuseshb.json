  "mysqldump": {
    "quick": "",
    "max_allowed_packet": "16M"
  },
  "mysql": {
    "no-auto-rehash": ""
  },
  "myisamchk": {
    "key_buffer_size": "512M",
    "sort_buffer_size": "512M",
    "read_buffer": "8M",
    "write_buffer": "8M"
  },
  "mysqlhotcopy": {
    "interactive-timeout": ""
  },
  "mysqld_safe": {
    "open-files-limit": "8192"
  }

{
  "client": {
    "port": "3306",
    "socket": "/tmp/mysql.sock",
    "collation-server": "utf8_unicode_ci",
    "character-set-server": "utf8"
  },
  "mysqld": {
    "port": "3306",
    "socket": "/tmp/mysql.sock",
    "collation-server": "utf8_unicode_ci",
    "character-set-server": "utf8",
    "default-time-zone": "'UTC'",
    "transaction_isolation": "READ-COMMITTED",
    "key_buffer_size": "32M",
    "innodb_buffer_pool_size": "128M",
    "innodb_file_per_table": "",

{
  "mysqld": {
    "server-id": "1",
    "log-bin": "mysql-bin",
    "binlog-do-db": "webdb,just_test",
    "binlog-ignore-db": "mysql",
    "datadir": "/var/lib/mysql",
    "socket": "/var/lib/mysql/mysql.sock",
    "user": "mysql",
    "symbolic-links": "0"
  },
  "mysqld_safe": {
    "log-error": "/var/log/mysqld.log",
    "pid-file": "/var/run/mysqld/mysqld.pid"
  }
}
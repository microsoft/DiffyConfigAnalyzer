{
  "client": {
    "port": "3306",
    "socket": "/var/run/mysqld/mysqld.sock"
  },
  "mysqldump": {
    "quick": "",
    "quote-names": "",
    "max_allowed_packet": "16M"
  },
  "mysqld_safe": {
    "socket": "/var/run/mysqld/mysqld.sock",
    "nice": "0",
    "log-error": "/var/log/mysql/error.log",
    "character-set-server": "utf8",
    "default-time-zone": "'+00:00'"
  },
  "mysqld": {
    "character-set-server": "utf8",
    "default-time-zone": "'+00:00'",
    "key_buffer_size": "256M",
    "join_buffer_size": "32M",
    "sort_buffer_size": "16M",
    "read_buffer_size": "4M",
    "read_rnd_buffer_size": "4M",
    "query_cache_size": "16M",
    "thread_concurrency": "4",
    "innodb_buffer_pool_size": "820M",
    "innodb_additional_mem_pool_size": "16M",
    "innodb_autoextend_increment": "128",
    "innodb_log_file_size": "500M",
    "innodb_log_buffer_size": "205M",
    "innodb_thread_concurrency": "4",
    "innodb_read_io_threads": "4",
    "innodb_write_io_threads": "4",
    "myisam_max_sort_file_size": "820M",
    "myisam_sort_buffer_size": "205M",
    "tmp_table_size": "128M",
    "max_heap_table_size": "128M",
    "table_open_cache": "1000",
    "default-storage-engine": "InnoDB",
    "innodb_file_per_table": "1",
    "innodb_file_format": "barracuda",
    "innodb_purge_threads": "1",
    "skip-name-resolve": "",
    "user": "mysql",
    "pid-file": "/var/run/mysqld/mysqld.pid",
    "socket": "/var/run/mysqld/mysqld.sock",
    "port": "3306",
    "basedir": "/usr",
    "datadir": "/var/lib/mysql",
    "tmpdir": "/tmp",
    "lc-messages-dir": "/usr/share/mysql",
    "skip-external-locking": "",
    "bind-address": "127.0.0.1",
    "max_allowed_packet": "16M",
    "thread_stack": "192K",
    "thread_cache_size": "4",
    "myisam-recover-options": "BACKUP",
    "query_cache_limit": "16M",
    "log-error": "/var/log/mysql/error.log",
    "slow-query-log": "ON",
    "slow_query_log_file": "/var/log/mysql/mysql-slow.log",
    "long_query_time": "2",
    "log-queries-not-using-indexes": "ON",
    "log-slow-admin-statements": "TRUE",
    "server-id": "1",
    "log-bin": "/var/log/mysql/mysql-bin.log",
    "expire_logs_days": "10",
    "sync_binlog": "1",
    "max_binlog_size": "100M",
    "binlog_format": "MIXED",
    "transaction_isolation": "READ-COMMITTED",
    "ssl-ca": "/etc/ssl/localcerts/AddTrustExternalCARoot.pem",
    "ssl-cert": "/etc/ssl/localcerts/makeyourlaws.org.combined.pem",
    "ssl-key": "/etc/ssl/localcerts/makeyourlaws.org.key_with_rsa",
    "secure-auth": "ON"
  },
  "mysql": {
    "disable-auto-rehash": ""
  },
  "isamchk": {
    "key_buffer_size": "16M",
    "!includedir /etc/mysql/conf.d/": ""
  }
}